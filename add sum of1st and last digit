using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp9
{
    class Program
    {
        static void Main(string[] args)
        {
            int n,r,sum=0;
            Console.Write("ENTER THE N VALUE :");
            n = int.Parse(Console.ReadLine());

            n = n / 10;
            while(n>10)
            {
                r = n % 10;
                sum = sum + r;
                n = n / 10;
            }
            Console.WriteLine(sum);
            Console.ReadLine();

        }
    }
}
